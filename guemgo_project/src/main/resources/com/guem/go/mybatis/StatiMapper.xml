<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guem.go.mybatis.StatiMapper">
	<select id="countusers" resultType="long">
		select count(*) from users
	</select>
	<select id="countgosu" resultType="long">
		select count(*) from gosu
	</select>
	<select id="countrequ" resultType="long">
		select count(*) from require
	</select>
	<select id="countmatch" resultType="long">
		select count(*) from matching
	</select>
	<select id="payselect" resultType="com.guem.go.eunbyul.PayVo">
		select * from pay
	</select>
</mapper>

